<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/WasherCurrentCycleid"
        android:layout_alignEnd="@+id/WasherDoorClosedid"
        android:layout_marginEnd="13dp"
        android:id="@+id/Cycleid"
        android:textColorLink="?attr/editTextColor"
        android:textSize="18sp"
        android:fontFamily="sans-serif-condensed" />

    <Button
        android:text="Set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="95dp"
        android:id="@+id/WasherSetid"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/WasherDoorClosedtoggleid" />

    <TextView
        android:text="DoorClosed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/WasherDoorClosedid"
        android:textColorLink="?attr/editTextColor"
        android:textSize="18sp"
        android:fontFamily="sans-serif-condensed"
        android:layout_alignBottom="@+id/WasherDoorClosedtoggleid"
        android:layout_alignEnd="@+id/DishWasherRunningid" />

    <TextView
        android:text="CurrentCycle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/WasherCurrentCycleid"
        android:textColorLink="?attr/editTextColor"
        android:textSize="18sp"
        android:fontFamily="sans-serif-condensed"
        android:layout_marginBottom="61dp"
        android:layout_above="@+id/WasherSetid"
        android:layout_alignEnd="@+id/WasherSetid"
        android:layout_marginEnd="68dp" />

    <TextView
        android:text="WasherRunning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="147dp"
        android:id="@+id/DishWasherRunningid"
        android:textColorLink="?attr/editTextColor"
        android:textSize="18sp"
        android:fontFamily="sans-serif-condensed"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/WasherSetid" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/WasherRunningid"
        android:layout_marginEnd="57dp"
        android:layout_alignBaseline="@+id/DishWasherRunningid"
        android:layout_alignBottom="@+id/DishWasherRunningid"
        android:layout_alignParentEnd="true" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:id="@+id/WasherDoorClosedtoggleid"
        android:layout_below="@+id/WasherRunningid"
        android:layout_alignStart="@+id/WasherRunningid" />
</RelativeLayout>