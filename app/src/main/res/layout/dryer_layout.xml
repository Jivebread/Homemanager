<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:text="LintTrapCleanid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/DryerCycleTypeid"
        android:layout_alignEnd="@+id/DryerCycleTypeid"
        android:layout_marginTop="45dp"
        android:id="@+id/LintTrapCleanid" />

    <Button
        android:text="Set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LintTrapCleanid"
        android:layout_toEndOf="@+id/LintTrapCleanid"
        android:layout_marginTop="88dp"
        android:id="@+id/DryerSetid" />

    <TextView
        android:text="DryerRunning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="93dp"
        android:layout_marginTop="137dp"
        android:id="@+id/DryerRunningid"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignEnd="@+id/DryerRunningToggleid"
        android:id="@+id/DryerCycleToggleid" />

    <TextView
        android:text="DryerCycleType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/DryerCycleTypeid"
        android:layout_centerVertical="true"
        />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/DryerRunningToggleid"
        android:layout_alignTop="@+id/DryerRunningid"
        android:layout_toEndOf="@+id/DryerSetid" />

    <TextView
        android:text="CycleType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/DryerCycleid1"
        android:layout_below="@+id/DryerRunningToggleid"
        android:layout_alignEnd="@+id/DryerRunningid"
        android:layout_marginEnd="50dp" />

    <TextView
        android:text="DryerCycleType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:id="@+id/DryerCycleTypeValueid"
        android:layout_alignBaseline="@+id/DryerCycleTypeid"
        android:layout_alignBottom="@+id/DryerCycleTypeid"
        android:layout_alignStart="@+id/lintTrapCleantoggleid" />

    <TextView
        android:text="None"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/LintTrapCleanid"
        android:layout_toEndOf="@+id/DryerSetid"
        android:id="@+id/LintTrapValueid" />

</RelativeLayout>